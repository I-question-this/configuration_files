#!/usr/bin/env bash

# Color Scheme
exec wal --vte -R &
# Document Management
exec dropbox &
exec zotero &
# Lock Screen
exec xss-lock -- ~/.bin/transfer-sleep-lock-i3lock.sh &
# Messaging
exec discord &
exec slack &
# Password Management
exec keepassxc &
# Web browsing
exec firefox &
# Window compositor (for transparency)
exec compton &
# Program for applying special rules for what is and isn't transparent
exec devilspie -a &
