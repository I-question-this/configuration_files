[meld]
	tool = meld
[alias]
  delbrs = fetch origin --prune
  newbr = !git pull && git checkout -b $1 && git push --set-upstream origin $1 &&:
  changes = diff @{u}
[pull]
	rebase = false
[init]
	defaultBranch = main
