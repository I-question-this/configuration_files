# General
AddKeysToAgent yes

host github
  IdentityFile ~/.ssh/github
  User git
  Hostname github.com

# Host specific
host biggamer
  ForwardAgent yes
  IdentityFile ~/.ssh/dexter-biggamer
  # Folding@Home
  LocalForward 7396 localhost:7396
  User tyler
  Hostname apartment.home

host biggamer-at-home
  ForwardAgent yes
  IdentityFile ~/.ssh/dexter-biggamer
  # Folding@Home
  LocalForward 7396 localhost:7396
  User tyler
  Hostname biggamer.apartment.home

host retropie
  User pi
  Hostname retropie.apartment.home

host dracarys
  ForwardAgent yes
  IdentityFile ~/.ssh/dracarys
  # phpMyAdmin
  LocalForward 8989 localhost:80
  # Folding@Home
  LocalForward 7396 localhost:7397
  User tylerwestland
  Hostname dracarys.mindlab.uc.edu

host metasploitable
  User msfadmin
  Hostname metasploitable.biggamer.apartment.home
  Port 22
  ProxyCommand ssh -q -W %h:%p biggamer

host kali
  User root
  Hostname kali.biggamer.apartment.home
  Port 2222
  ProxyCommand ssh -q -W %h:%p biggamer

